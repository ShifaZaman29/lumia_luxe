{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/nextjs"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://lumialuxe-production.up.railway.app/api"
  },
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs"
}